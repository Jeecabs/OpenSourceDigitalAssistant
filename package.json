{
  "name": "jarvis",
  "version": "0.2.0",
  "description": "Server, packages and web app of the Jarvis personal assistant",
  "author": {
    "name": "L"
  },
  "license": "MIT",
  "scripts": {
    "lint": "babel-node scripts/lint.js",
    "setup:offline": "ts-node scripts/setup-offline/setup-offline.ts",
    "setup:offline-stt": "bash DOWNLOAD_DEEPSPEECH.sh",
    "setup:offline-tts": "bash TTS.sh",
    "setup:offline-hotword": "babel-node scripts/setup-offline/run-setup-hotword.js",
    "preinstall": "node scripts/setup/preinstall.js",
    "dev:app": "babel-node scripts/app/dev-app.js",
    "dev:server": "npm run train expressions && cross-env LEON_NODE_ENV=development nodemon --watch server ./server/src/index.js --ignore server/src/tmp/ --exec babel-node",
    "wake": "cross-env LEON_HOST=http://localhost LEON_PORT=1337 ts-node hotword/index.ts",
    "build": "npm run lint && npm run build:app && npm run build:server",
    "build:app": "cross-env LEON_NODE_ENV=production babel-node scripts/app/run-build-app.js",
    "build:server": "npm run delete-dist:server && npm run train expressions && babel ./server/src -d ./server/dist --copy-files && shx mkdir -p server/dist/tmp",
    "start": "LEON_NODE_ENV=production  && ts-node server/src/index.ts",
    "train": "ts-node scripts/run-train.ts",
    "python:start": "cd TTS_Python && uvicorn main:app --reload",
    "openapi": "bash GENERATE_OPENAPI.sh"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.0.20",
    "@ffprobe-installer/ffprobe": "^1.0.9",
    "@google-cloud/speech": "^3.2.0",
    "@google-cloud/text-to-speech": "^1.1.0",
    "@types/archiver": "^5.1.0",
    "@types/cli-spinner": "^0.2.0",
    "@types/cors": "^2.8.9",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/fluent-ffmpeg": "^2.1.16",
    "@types/node-fetch": "^2.5.8",
    "@types/socket.io": "^2.1.13",
    "archiver": "^5.2.0",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "deepspeech": "^0.9.3",
    "dotenv": "^8.2.0",
    "express": "^4.15.3",
    "fluent-ffmpeg": "^2.1.2",
    "googleapis": "^40.0.0",
    "ibm-watson": "^4.1.3",
    "jshint": "^2.12.0",
    "moment-timezone": "^0.5.14",
    "node-fetch": "^2.6.1",
    "node-nlp": "^3.1.1",
    "node-wav": "0.0.2",
    "socket.io": "^3.1.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "cli-spinner": "^0.2.8",
    "git-changelog": "git+https://github.com/Jeecabs/Jarvis",
    "husky": "^0.14.3",
    "inquirer": "^5.1.0",
    "nodemon": "^1.18.9"
  }
}
